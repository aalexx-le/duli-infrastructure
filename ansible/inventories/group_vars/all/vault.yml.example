---
# This file contains sensitive variables encrypted with Ansible Vault
# Copy to vault.yml and fill in your values, then encrypt with:
# ansible-vault encrypt ansible/inventories/cluster/group_vars/all/vault.yml

# DigitalOcean API Token (required for CSI driver)
# Get your token from: https://cloud.digitalocean.com/account/api/tokens
vault_do_api_token: "your-digitalocean-api-token"

# Database passwords
vault_postgres_password: "your-secure-postgres-password"
vault_redis_password: "your-secure-redis-password"
vault_rabbitmq_password: "your-secure-rabbitmq-password"
